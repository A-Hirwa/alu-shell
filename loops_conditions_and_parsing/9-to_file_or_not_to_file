#!/usr/bin/env bash
#information on school file
filename="school"

if [ -e $filename ];then
    echo "File exists"
else
    echo "File doesn't exist"
fi
if [ -z $filename ];then
	echo "school file is empty"
elif [ -s $filename ];then
	echo "school file is not empty"
elif [ -f $filename ];then
	echo "school is a regular files."
else
	echo "(nothing)"
fi
